ar Arquivo 6","credentials":{"sshPassword":{"id":"PLqwZKiY3geSh8YV","name":"SSH Password account"}},"onError":"continueErrorOutput"},{"parameters":{"jsCode":"// pega o id dinâmico\nconst id = $('Processar Próximo Cartão').first().json.id;\nconst prefix = `/tmp/${id}`;\n\n// monta a lista de WAVs com o prefixo\nconst files = Array.from({ length: 6 }, (_, i) => `${prefix}tmpaudio${i+1}.wav`);\n\n// concat list e output também ganham o prefixo\nconst listFilePath = `${prefix}_concat_list.txt`;\nconst outputPath   = `${prefix}combined_output.mp3`;\n\n// gera o conteúdo do arquivo de lista\nconst fileListContent = files.map(f => `file '${f}'`).join('\\n');\n\n// monta o script bash\nconst bashScript = [\n  `rm -f ${outputPath}`,                                   // limpa MP3 antigo\n  `cat <<EOF > ${listFilePath}`,\n  fileListContent,\n  `EOF`,\n  `ffmpeg -y -f concat -safe 0 -i ${listFilePath} -c:a libmp3lame -qscale:a 2 ${outputPath}`, // concat + MP3\n  `rm -f ${files.join(' ')}`                                // apaga os WAVs\n].join('\\n');\n\nreturn [\n  {\n    json: {\n      bashScript,\n    },\n  },\n];\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[216,2815],"id":"8fb475e2-ed56-47aa-b321-f808a85a0059","name":"Preparar Script de Concatenação"},{"parameters":{"command":"={{ $json.bashScript }}"},"type":"n8n-nodes-base.ssh","typeVersion":1,"position":[436,2815],"id":"67762692-a086-465e-837b-e86e62f7fe9f","name":"Executar Concatenação de Áudio","credentials":{"sshPassword":{"id":"PLqwZKiY3geSh8YV","name":"SSH Password account"}}},{"parameters":{"resource":"file","operation":"download","path":"=/tmp/{{ $item(\"0\").$node[\"Processar Próximo Cartão\"].json[\"id\"] }}combined_output.mp3","options":{}},"type":"n8n-nodes-base.ssh","typeVersion":1,"position":[656,2815],"id":"0040f673-d7dc-46d5-8228-f6751019c7b3","name":"Baixar Áudio Combinado","credentials":{"sshPassword":{"id":"PLqwZKiY3geSh8YV","name":"SSH Password account"}}},{"parameters":{"name":"output