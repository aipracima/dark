Arr[0].seo = text;\n  }\n});\n\n// Retorna os items já limpos\nreturn items;\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-3580,-260],"id":"8d07ae05-ed79-4091-b7ab-662aabe9b271","name":"Limpar Texto da Descrição"},{"parameters":{"operation":"get","id":{"__rl":true,"value":"={{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}","mode":"id"},"additionalFields":{}},"type":"n8n-nodes-base.trello","typeVersion":1,"position":[-2920,-260],"id":"c082eb87-8e3e-4e20-9e83-c4a451cdec63","name":"Obter Detalhes do Cartão","credentials":{"trelloApi":{"id":"6gLxfTJw8EfH7C8E","name":"Trello account"}}},{"parameters":{"operation":"update","id":{"__rl":true,"value":"={{ $json.id }}","mode":"id"},"updateFields":{"desc":"=\"título\": \"{{ $item(\"0\").$node[\"Consolidar Dados do Cartão\"].json[\"output\"][\"0\"][\"título\"] }}\"\n\"pasta\": \"{{ $item(\"0\").$node[\"Consolidar Dados do Cartão\"].json[\"output\"][\"0\"][\"pasta\"] }}\",\n\"id\": \"{{ $item(\"0\").$node[\"Enviar para API de Upload do Google Drive\"].json[\"file\"][\"id\"] }}\",\n\"url\": \"{{ $item(\"0\").$node[\"Enviar para API de Upload do Google Drive\"].json[\"file\"][\"webViewLink\"] }}\",\n\"data\": \"{{ $item(\"0\").$node[\"Calcular Data de Publicação\"].json[\"datePlusOne\"] }}\",\n"}},"type":"n8n-nodes-base.trello","typeVersion":1,"position":[-2700,-260],"id":"04026c4f-560a-4afa-992e-a86658adb0c9","name":"Atualizar Cartão com Link do Vídeo","credentials":{"trelloApi":{"id":"6gLxfTJw8EfH7C8E","name":"Trello account"}}},{"parameters":{"command":"=rm -f \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}concatenated_output.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo1.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo2.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo3.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecio