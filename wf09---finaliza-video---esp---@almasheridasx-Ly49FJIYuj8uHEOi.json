lor no JSON de saída\n    outputArr[0].seo = text;\n  }\n});\n\n// Retorna os items já limpos\nreturn items;\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-3140,-260],"id":"4a78ad1f-7ae4-4f38-aca4-c452c55557b9","name":"Limpar Texto da Descrição"},{"parameters":{"operation":"get","id":{"__rl":true,"value":"={{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}","mode":"id"},"additionalFields":{}},"type":"n8n-nodes-base.trello","typeVersion":1,"position":[-2480,-260],"id":"f1e76e74-6fd8-4e00-a611-a8a522b94d77","name":"Obter Detalhes do Cartão","credentials":{"trelloApi":{"id":"6gLxfTJw8EfH7C8E","name":"Trello account"}}},{"parameters":{"operation":"update","id":{"__rl":true,"value":"={{ $json.id }}","mode":"id"},"updateFields":{"desc":"=\"título\": \"{{ $item(\"0\").$node[\"Consolidar Dados do Cartão\"].json[\"output\"][\"0\"][\"título\"] }}\"\n\"pasta\": \"{{ $item(\"0\").$node[\"Consolidar Dados do Cartão\"].json[\"output\"][\"0\"][\"pasta\"] }}\",\n\"id\": \"{{ $item(\"0\").$node[\"Enviar para API de Upload do Google Drive\"].json[\"file\"][\"id\"] }}\",\n\"url\": \"{{ $item(\"0\").$node[\"Enviar para API de Upload do Google Drive\"].json[\"file\"][\"webViewLink\"] }}\",\n\"data\": \"{{ $item(\"0\").$node[\"Calcular Data de Publicação\"].json[\"datePlusOne\"] }}\",\n"}},"type":"n8n-nodes-base.trello","typeVersion":1,"position":[-2260,-260],"id":"3170a1eb-1c89-44a6-8055-54a6dff82849","name":"Atualizar Cartão com Link do Vídeo","credentials":{"trelloApi":{"id":"6gLxfTJw8EfH7C8E","name":"Trello account"}}},{"parameters":{"command":"=rm -f \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}concatenated_output.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo1.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo2.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo3.mp4\" \\       \"/tmp