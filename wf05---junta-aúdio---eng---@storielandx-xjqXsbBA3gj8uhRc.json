 Arquivo 6","credentials":{"sshPassword":{"id":"PLqwZKiY3geSh8YV","name":"SSH Password account"}},"onError":"continueErrorOutput"},{"parameters":{"jsCode":"// pega o id dinâmico\nconst id = $('Processar Próximo Cartão').first().json.id;\nconst prefix = `/tmp/${id}`;\n\n// monta a lista de WAVs com o prefixo\nconst files = Array.from({ length: 6 }, (_, i) => `${prefix}tmpaudio${i+1}.wav`);\n\n// concat list e output também ganham o prefixo\nconst listFilePath = `${prefix}_concat_list.txt`;\nconst outputPath   = `${prefix}combined_output.mp3`;\n\n// gera o conteúdo do arquivo de lista\nconst fileListContent = files.map(f => `file '${f}'`).join('\\n');\n\n// monta o script bash\nconst bashScript = [\n  `rm -f ${outputPath}`,                                   // limpa MP3 antigo\n  `cat <<EOF > ${listFilePath}`,\n  fileListContent,\n  `EOF`,\n  `ffmpeg -y -f concat -safe 0 -i ${listFilePath} -c:a libmp3lame -qscale:a 2 ${outputPath}`, // concat + MP3\n  `rm -f ${files.join(' ')}`                                // apaga os WAVs\n].join('\\n');\n\nreturn [\n  {\n    json: {\n      bashScript,\n    },\n  },\n];\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[216,2815],"id":"db36ed7a-16fd-4303-8bfc-1cdc4b25f119","name":"Preparar Script de Concatenação"},{"parameters":{"command":"={{ $json.bashScript }}"},"type":"n8n-nodes-base.ssh","typeVersion":1,"position":[436,2815],"id":"2ff4337e-5d86-4125-8b8d-8775d1d90cb7","name":"Executar Concatenação de Áudio","credentials":{"sshPassword":{"id":"PLqwZKiY3geSh8YV","name":"SSH Password account"}}},{"parameters":{"resource":"file","operation":"download","path":"=/tmp/{{ $item(\"0\").$node[\"Processar Próximo Cartão\"].json[\"id\"] }}combined_output.mp3","options":{}},"type":"n8n-nodes-base.ssh","typeVersion":1,"position":[656,2815],"id":"cd681366-e20c-46cd-bf6d-ec6350f36e1d","name":"Baixar Áudio Combinado","credentials":{"sshPassword":{"id":"PLqwZKiY3geSh8YV","name":"SSH Password account"}}},{"parameters":{"name":"output",